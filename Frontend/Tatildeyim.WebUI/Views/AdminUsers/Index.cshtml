@model List<ResultAppUserDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="active-member">
                    <div class="table-responsive">
                        <table class="table table-xs mb-0">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>Ad Soyad</th>
                                    <th>Şehir</th>
                                    <th>Durum</th>
                                    <th>Sil</th>
                                    <th>Güncelle</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach(var item in Model){
                                    <tr>
                                        <td><img src="@item.ImageUrl" class=" rounded-circle mr-3" alt=""> </td>
                                        <td>@item.Name @item.Surname</td>
                                        <td><span>@item.City</span></td>
                                        <td><i class="fa fa-circle-o text-success  mr-2"></i> Aktif</td>
                                        <td><i class="fa fa-delete text-success  mr-2"></i> Sil</td>
                                        <td><i class="fa fa-reply text-success  mr-2"></i> Güncelle</td>
                                    </tr>
                                }
                                
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

